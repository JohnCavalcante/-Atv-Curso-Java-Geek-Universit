import java.util.Scanner;

public class Ex3_Estrutura_sequencial {

	public static void main(String[] args) {
		Scanner entrada = new Scanner(System.in);
		
		int A, B, C, D;
		 A = entrada.nextInt();
		 B = entrada.nextInt();
		 C = entrada.nextInt();
		 D = entrada.nextInt();
		
		int E = A * B - C * D;
		
		System.out.println("DIFERENCA =" + E); 
		
		entrada.close();
	}
}
